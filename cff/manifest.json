{
  "name": "Crowd Fact Checker",
  "description": "BAKK Arbeit",
  "version": "1.1",
  "manifest_version": 6,
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "contextMenus",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "persistent": false,
    "scripts": ["text_util.js", "gerie_queue.js", "histogram.js", "levenshtein.js", "evaluator.js", "messenger.js",
      "fact_collector.js", "background.js", "process_question.js"]
  }
}